basic_config {
  env {
    parameters {
      key: "TF_CPP_MIN_LOG_LEVEL"
      value: "2"
    }
  }
  gpu_config {
    parameters {
      key: "devices"
      value: "auto"
    }
  }
  model_repository: "./models/model_repository"
  http_port: 8502
  grpc_port: 6000
  metrics_port: 6002
  log_verbose: 0
  log_file: "./logs/bisheng-rt.log"
}

app {
  name: "idp_app"
  path: "ocr_app"
  instance_group {
    count: 10
    kind: KIND_CPU
  }
}